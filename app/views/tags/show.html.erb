<% @nav_id = "nav-item" %>

<% content_for :left_panel do %>
  &nbsp;
<% end %>

<% content_for :right_panel do %>
  &nbsp;
<% end %>


<div class="row">
  <div class="span6">
    <h3>Wingmen in <%= @tag.name %></h3>
    <hr />
    <% unless @people.empty? %>
      <% @people.each do |p| %>
        <%= render "/people/person_small", :person => p %>
      <% end %>
    <% else %>
      This tag doesn't have any wingmen
    <% end %>
  </div>
</div>
